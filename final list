import java.util.ArrayList;
import java.util.List;

public class FinalArrayList {
    public static void main(String[] args) {
        final List<Integer> list = new ArrayList<>(5);
        list.add(1);
        list.add(2);
        list.add(3);
        list.add(4);
        list.add(5);
        list.add(6);
        list.add(7);

        //list = List.of(1);//immutable, meaning their size and elements cannot be changed after creation

        System.out.println(list);
    }
